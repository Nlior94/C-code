#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

//方法一
int main()
{
	int m = 0;
	int n = 0;
	scanf("%d%d", &m, &n);

	int max = 0;
	if (m > n)
	    max = m;
	else
		max = n;

	while (1)
	{
		if (m % max == 0 && n % max == 0)
		{
			printf("最大公约数为：%d\n", max);
			break;
		}
		else
			max--;
	}
	return 0;
}

//方法二（辗转相除法）
int main()
{
	int m = 0;
	int n = 0;
	scanf("%d%d", &m, &n);
	int t = 0;

	while (m % n)//不用比较大小，循环内的相互赋值已经帮助调整了
	{
		t = m % n;
		m = n;
		n = t;
	}
	printf("最大公约数为；%d\n", n);

	return 0;
}
